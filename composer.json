{
  "name": "mosiyash/hideme-parser",
  "description": "Parser for hideme.ru",
  "keywords": ["hideme.ru", "hidemyna.me", "proxy", "guzzlehttp", "parser"],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/dom-crawler": "^4.1",
    "symfony/css-selector": "^4.1",
    "symfony/cache": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7 || ~4.8",
    "mmucklo/krumo": "^0.5.7",
    "friendsofphp/php-cs-fixer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Mosiyash\\HidemeParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mosiyash\\HidemeParser\\": "tests/"
    }
  }
}
